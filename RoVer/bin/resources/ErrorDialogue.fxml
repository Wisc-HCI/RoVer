<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="200.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="450.0" style="-fx-background-color: white;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Text fx:id="errorText" boundsType="VISUAL" strokeType="OUTSIDE" strokeWidth="0.0" text="Error" textAlignment="CENTER">
               <font>
                  <Font name="Calibri Light" size="25.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="50.0" left="25.0" right="25.0" top="50.0" />
               </VBox.margin>
            </Text>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="350.0" spacing="50.0">
               <children>
                  <Button fx:id="okayButton" alignment="CENTER" mnemonicParsing="false" prefWidth="100.0" text="Okay" textAlignment="CENTER" />
                  <Button fx:id="detailsButton" alignment="CENTER" mnemonicParsing="false" prefWidth="100.0" text="Details &gt;&gt;" textAlignment="CENTER" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
