<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="350.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox prefHeight="350.0" prefWidth="500.0" style="-fx-background-color: white;">
         <children>
            <TreeView fx:id="treeView" prefHeight="350.0" prefWidth="500.0" />
            <Separator prefWidth="200.0" />
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="150.0" style="-fx-background-color: white;">
               <children>
                  <Button fx:id="cancelButton" mnemonicParsing="false" text="Cancel" />
                  <Button fx:id="importButton" mnemonicParsing="false" text="Import" />
               </children>
               <VBox.margin>
                  <Insets bottom="25.0" left="50.0" right="50.0" top="25.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
